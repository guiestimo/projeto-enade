@model IEnumerable<ProjetoEnade.Models.QuestaoGabarito>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Provas)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Enunciado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RespostaA)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RespostaB)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RespostaC)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RespostaD)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RespostaE)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RespostaCorreta)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Provas.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Enunciado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RespostaA)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RespostaB)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RespostaC)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RespostaD)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RespostaE)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RespostaCorreta)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
